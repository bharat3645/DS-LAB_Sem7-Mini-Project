# Secrets Configuration Template
# Copy this file to .streamlit/secrets.toml and fill in your values
# DO NOT commit secrets.toml to version control

# Example: Database credentials (if needed in future)
# [database]
# host = "your-database-host"
# port = 5432
# database = "your-database-name"
# username = "your-username"
# password = "your-password"

# Example: API Keys (if needed)
# [api_keys]
# google_maps = "your-google-maps-api-key"
# openai = "your-openai-api-key"

# Example: Custom configurations
# [app_config]
# admin_email = "admin@example.com"
# max_upload_size = 200
